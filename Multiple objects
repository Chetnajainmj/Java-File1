//
import java.util.*;
public class A implements Runnable
{
	public void run(){
		    try{
		  Thread.sleep(1000);
		    System.out.println(" try run");
		}
		catch(Exception e){
		     System.out.println(" catch run");
		}}
		public static void main(String[] args) {
		    for(int i=0;i<10;i++){
		        Thread t1=new Thread( new A());
		        t1.start();}
		    }}
