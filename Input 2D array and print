import java.util.*;
public class LinearsearchExample{

	public static void main(String[] args) {
	     
	    int a1[][]=new int[4][3];
	 Scanner sc=new Scanner(System.in);
	     for(int i=0;i<4;i++){
	         for(int j=0;j<3;j++){
	                a1[i][j]=sc.nextInt();
	    }   
	
      }
       for(int i=0;i<4;i++){
	         for(int j=0;j<3;j++){
	                
	    System.out.print(a1[i][j]+" ");}   
	  System.out.println();
      }  
	  
	}}
